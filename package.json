{
	"name": "number.prototype.toexponential",
	"version": "1.0.3",
	"description": "An ES-spec-compliant Number.prototype.toExponential shim/polyfill/replacement that works as far down as ES3",
	"main": "index.js",
	"exports": {
		".": [
			{
				"import": "./index.mjs",
				"require": "./index.js",
				"default": "./index.js"
			},
			"./index.js"
		],
		"./auto": "./auto.js",
		"./polyfill": "./polyfill.js",
		"./implementation": "./implementation.js",
		"./shim": "./shim.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"prepublishOnly": "safe-publish-latest",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"pretest": "npm run lint",
		"lint": "eslint --ext=js,mjs .",
		"postlint": "es-shim-api --bound",
		"tests-only": "nyc tape 'test/**/*.js'",
		"tests-esm": "nyc node test/index.mjs",
		"test": "npm run tests-only && npm run tests-esm",
		"posttest": "aud --production",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/es-shims/Number.prototype.toExponential.git"
	},
	"keywords": [
		"ecmascript",
		"javascript",
		"polyfill",
		"shim",
		"number",
		"exponential",
		"toexponential"
	],
	"author": "Jordan Harband <ljharb@gmail.com>",
	"funding": {
		"url": "https://github.com/es-shims/Number.prototype.toExponential?sponsor=1"
	},
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/es-shims/Number.prototype.toExponential/issues"
	},
	"homepage": "https://github.com/es-shims/Number.prototype.toExponential#readme",
	"devDependencies": {
		"@es-shims/api": "^2.2.3",
		"@ljharb/eslint-config": "^20.0.0",
		"aud": "^1.1.5",
		"auto-changelog": "^2.3.0",
		"eslint": "^8.4.1",
		"functions-have-names": "^1.2.2",
		"nyc": "^10.3.2",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.3.2"
	},
	"dependencies": {
		"call-bind": "^1.0.2",
		"define-properties": "^1.1.3",
		"es-abstract": "^1.19.1",
		"math.log10": "^1.0.0"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true
	},
	"testling": {
		"files": "test/shimmed.js"
	}
}
